<?xml version="1.0" encoding="ISO-8859-1" ?> 
 <descriptors>
 <descriptor>
 <!-- type could be relational:mysql  or relational:postgre  and so on -->
 <!--  type could be custom:className   The ClassName being the object which is responsible for getting results-->
<datasource name="DS1">
<table name="DS1_Synonyms">
  <column name="Address" type="varchar" /> 
  <column name="Entry" type="varchar">
  <!--  the base is used to construct a URI for the value stored in the table
  e.g if the table stores a value status. It would correspond to URI ${base}/status
   -->
  <operation type="AVH" base="http://localhost/bookExample/ont1.owl" ontology="http://localhost/bookExample/ont1.owl" ontLoc="ont1.owl" /> 
  </column>
  <column name="ISBN" type="varchar" /> 
  <column name="Name" type="varchar" /> 
  <column name="Organization" type="varchar"> 
  <!--  the base is used to construct a URI for the value stored in the table
  e.g if the table stores a value status. It would correspond to URI ${base}/status
   -->
  <operation type="AVH" base="http://localhost/bookExample/ont1.owl" ontology="http://localhost/bookExample/ont1.owl" ontLoc="ont1.owl" /> 
  </column>
  <column name="Person" type="char" /> 
  <column name="PublishedOn" type="date" /> 
  </table>
  </datasource>
  </descriptor>
  </descriptors>

